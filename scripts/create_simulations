#!/bin/bash


(
cd "/research/googledrive/MacIver Lab: Habitat/Data/PEEK/"
for FILE in * ; do
  if [ -d "$FILE" ]; then
    echo $FILE
    (
      cd "$FILE"
      EXPERIMENT_FILE="${FILE}_experiment_full.json"
      SIMULATION_FILE="${FILE}_simulation.json"

      echo "Experiment file: $EXPERIMENT_FILE"
      echo "Simulation file: $SIMULATION_FILE"
      if [ ! -f "$SIMULATION_FILE.json" ]; then
        /mnt/c/Research/cellworld_planner/cmake-build-debug/create_simulation -e ${EXPERIMENT_FILE} -s ${SIMULATION_FILE} -c /mnt/c/Research/cellworld_planner/config/belief_state_creation.json
      fi
    )
  fi
done
)