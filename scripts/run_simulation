#!/bin/bash

configuration="$1"
folder="$2"
folder="${CELLWORLD_PLANNER}/simulation_results/$2"

for w1 in {100..599}
do
  for e in {0..9}
  do
    world="${w1}_0${e}"
    if ls $folder/${configuration}.${world}.* 1> /dev/null 2>&1
    then
      #simulation results found
      echo "${world} found"
    else
      #runs the simulation
      $CELLWORLD_PLANNER_BIN/random_world_sim -wc ${configuration} -o $world ${@:3} -out $folder

    fi
  done
done